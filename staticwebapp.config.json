{
  "routes": [
    {
      "route": "/periods*",
      "methods": [
        "get"
      ]
    },
    {
      "route": "/transactions*",
      "methods": [
        "get"
      ]
    },
    {
      "route": "/categories",
      "methods": [
        "get"
      ]
    },
    {
      "route": "/accounts",
      "methods": [
        "get"
      ]
    },
    {
      "route": "/.auth/login/aad",
      "redirect": "/"
    },
    {
      "route": "/.auth/logout"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*"
    ]
  }
}